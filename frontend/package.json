{
  "name": "bql-frontend-workspace",
  "private": true,
  "packageManager": "bun@1.3.4",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "build": "bun run --filter '*' build",
    "typecheck": "bun run --filter '*' typecheck",
    "test": "bun run --elide-lines=0 test"
  }
}
